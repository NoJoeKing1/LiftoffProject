@model List<ParkRec.Models.Tag>

<h1>All Park Tags</h1>

@if(Model.Count == 0)
{
    <p>No tags yet!</p>
}
else
{
    <table class="table">
        <tr>
            <th>Id</th>
            <th>TagName</th>
        </tr>
        @foreach(var tag in Model)
        {
            <tr>
                <td>@tag.Id</td>
                <td>@tag.Name</td>
            </tr>
        }
    </table>
}
